<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BalloonRibbonAnimationView">
        <attr name="animation_speed">
            <enum name="slow" value="0" />
            <enum name="normal" value="1" />
            <enum name="fast" value="2" />
        </attr>

        <attr name="animation_type">
            <enum name="small" value="0" />
            <enum name="medium" value="1" />
            <enum name="large" value="2" />
        </attr>

        <attr name="animation_direction">
            <enum name="random" value="0" />
            <enum name="topToBottom" value="1" />
            <enum name="diagonal" value="2" />
        </attr>

        <attr name="animation_style">
            <enum name="standard" value="0" />
            <enum name="fireworks" value="1" />
        </attr>

    </declare-styleable>
</resources>